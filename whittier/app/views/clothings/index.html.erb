<h1 class="title is-1">
  This is Whittier
</h1>

<details>
  <summary>Shopping Cart</summary>
  <ul>
    <%= @cart.each do |product| %>
      <li><%= product.name %></li>
    <% end %>
  </ul>
</details>

<%= debug session[:cart]%>
<% if current_user %>
  <%= link_to 'Sign Out', destroy_user_session_path, method: :delete %>
<% else %>
  <%= link_to 'Sign In', new_user_session_path %>
  <%= link_to 'Sign Up', new_user_registration_path %>
<% end %>


<br />
<section class="section">
  <div class="container">
    <div class="columns">
      <%= render partial: 'clothingcard',
        object: @clothes
      %>
    </div>
  </div>
</section>
